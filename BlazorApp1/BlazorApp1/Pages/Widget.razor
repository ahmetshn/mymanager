@layout EmptLayout
@page "/widget"

<section @ref="_container" class="design-surface">
    <Canvas @ref="_tempCanvas"
            @onmousemove="MouseMoveCanvasAsync"
            @onmouseup="MouseUpCanvas"
            @onmousedown="MouseDownCanvas"
            width="@_width" height="@_height"></Canvas>

    <Canvas @ref="_mainCanvas"
            @onmousemove="MouseMoveCanvasAsync"
            @onmouseup="MouseUpCanvas"
            @onmousedown="MouseDownCanvas"
            width="@_width" height="@_height"></Canvas>
</section>

<div class="bg-secondary" style="position:absolute;z-index:99;top:0;left:0;bottom:0;width:50px;">
    <table class="table">
        <tbody>
            <tr>
                <td class="p-0 m-0">
                    <button type="button" @onclick="@(e => ChangeTool("pencil",false,true,true,false))" class="btn p-0 m-0" width="40" height="40">
                        <img class="p-0 m-0" width="40" height="40" src="data:image/png;base64," />
                    </button>
                </td>
            </tr>
            <tr>
                <td class="p-0 m-0">
                    <button type="button" @onclick="@(e => ChangeTool("marker",false,true,true,false))" class="btn p-0 m-0" width="40" height="40">
                        <img class="p-0 m-0" width="40" height="40" src="data:image/png;base64," />
                    </button>
                </td>
            </tr>
            <tr>
                <td class="p-0 m-0">
                    <button type="button" @onclick="@(e => ChangeTool("eraser",false,true,false,false))" class="btn p-0 m-0" width="40" height="40">
                        <img class="p-0 m-0" width="40" height="40" src="data:image/png;base64," />
                    </button>
                </td>
            </tr>
            <tr>
                <td class="p-0 m-0">
                    <button type="button" @onclick="@(e => ChangeTool("line",true,true,true,false))" class="btn p-0 m-0" width="40" height="40">
                        <img class="p-0 m-0" width="40" height="40" src="data:image/png;base64," />
                    </button>
                </td>
            </tr>
            <tr>
                <td class="p-0 m-0">
                    <button type="button" @onclick="@(e => ChangeTool("arrow",true,true,true,true))" class="btn p-0 m-0" width="40" height="40">
                        <img class="p-0 m-0" width="40" height="40" src="data:image/png;base64," />
                    </button>
                </td>
            </tr>
            <tr>
                <td class="p-0 m-0">
                    <button type="button" @onclick="@(e => ChangeTool("rectangle",true,true,true,true))" class="btn p-0 m-0" width="40" height="40">
                        <img class="p-0 m-0" width="40" height="40" src="data:image/png;base64," />
                    </button>
                </td>
            </tr>
            <tr>
                <td class="p-0 m-0">
                    <button type="button" @onclick="@(e => ChangeTool("undo",false,false,false,false))" class="btn p-0 m-0" width="40" height="40">
                        <img class="p-0 m-0" width="40" height="40" src="data:image/png;base64," />
                    </button>
                </td>
            </tr>
        </tbody>
    </table>
</div>

<div class="bg-secondary" style="position:absolute;z-index:99;top:0;left:50px;right:0;">
    <table class="table border-0 p-0 m-0 align-middle text-center">
        <tbody>
            <tr>
                @{
                    var lastCellWidth = 100;
                    if (_showLineWidth)
                    {
                        lastCellWidth -= 33;
                        <td style="width:33%" class="border-0 p-0 m-0 align-middle text-center">
                            <table class="table border-0 p-0 m-0">
                                <tbody>
                                    <tr>
                                        <td class="border-0 p-0 m-0 align-middle text-center">
                                            <button type="button"
                                                @onclick="@(e => ChangeLineWidht(1))"
                                            class="btnPencilLineWidth btn m-0 p-0 border border-dark" style="font-size: 1rem">
                                                &bull;
                                            </button>
                                        </td>
                                        <td class=" border-0 p-0 m-0 align-middle text-center">
                                            <button type="button"
                                                @onclick="@(e => ChangeLineWidht(2))"
                                            class="btnPencilLineWidth btn m-0 p-0" style="font-size: 1.25rem">
                                                &bull;
                                            </button>
                                        </td>
                                        <td class="border-0 p-0 m-0 align-middle text-center">
                                            <button type="button"
                                                @onclick="@(e => ChangeLineWidht(3))"
                                            class="btnPencilLineWidth btn m-0 p-0" style="font-size: 1.5rem">
                                                &bull;
                                            </button>
                                        </td>
                                        <td class="border-0 p-0 m-0 align-middle text-center">
                                            <button type="button"
                                                @onclick="@(e => ChangeLineWidht(4))"
                                            class="btnPencilLineWidth btn m-0 p-0" style="font-size: 1.75rem">
                                                &bull;
                                            </button>
                                        </td>
                                        <td class="border-0 p-0 m-0 align-middle text-center">
                                            <button type="button"
                                                @onclick="@(e => ChangeLineWidht(6))"
                                            class="btnPencilLineWidth btn m-0 p-0" style="font-size: 2rem">
                                                &bull;
                                            </button>
                                        </td>
                                        <td class="border-0 p-0 m-0 align-middle text-center">
                                            <button type="button"
                                                @onclick="@(e => ChangeLineWidht(8))"
                                            class="btnPencilLineWidth btn m-0 p-0" style="font-size: 2.25rem">
                                                &bull;
                                            </button>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </td>
                    }

                    if (_showLineColor)
                    {
                        lastCellWidth -= 33;
                        <td style="width:33%" class="border-0 p-0 m-0 align-middle text-center">
                            <table class="table border-0 p-0 m-0">
                                <tbody>
                                    <tr>
                                        <td class="border-0 p-0 m-0 align-middle text-center">
                                            |
                                        </td>
                                        <td class="border-0 p-0 m-0 align-middle text-center">
                                            <button type="button"
                                                @onclick="@(e => ChangeLineColor("07090d"))"
                                            class="bntRectangleBorderStyle btn m-0 p-0"
                                            style="font-size: 2.25rem;color: #07090d">
                                                &bull;
                                            </button>
                                        </td>
                                        <td class="border-0 p-0 m-0 align-middle text-center">
                                            <button type="button"
                                                @onclick="@(e => ChangeLineColor("2780b6"))"
                                            class="bntRectangleBorderStyle btn m-0 p-0 border border-dark"
                                            style="font-size: 2.25rem;color: #2780b6">
                                                &bull;
                                            </button>
                                        </td>
                                        <td class="border-0 p-0 m-0 align-middle text-center">
                                            <button type="button"
                                                @onclick="@(e => ChangeLineColor("c2372b"))"
                                            class="bntRectangleBorderStyle btn m-0 p-0"
                                            style="font-size: 2.25rem;color: #c2372b">
                                                &bull;
                                            </button>
                                        </td>
                                        <td class="border-0 p-0 m-0 align-middle text-center">
                                            <button type="button"
                                                @onclick="@(e => ChangeLineColor("26ad5c"))"
                                            class="bntRectangleBorderStyle btn m-0 p-0 "
                                            style="font-size: 2.25rem;color: #26ad5c">
                                                &bull;
                                            </button>
                                        </td>
                                        <td class="border-0 p-0 m-0 align-middle text-center">
                                            <button type="button"
                                                @onclick="@(e => ChangeLineColor("f6c812"))"
                                            class="bntRectangleBorderStyle btn m-0 p-0"
                                            style="font-size: 2.25rem;color: #f6c812">
                                                &bull;
                                            </button>
                                        </td>
                                        <td class="border-0 p-0 m-0 align-middle text-center">
                                            <button type="button"
                                                @onclick="@(e => ChangeLineColor("fbf9f9"))"
                                            class="bntRectangleBorderStyle btn m-0 p-0"
                                            style="font-size: 2.25rem;color: #fbf9f9">
                                                &bull;
                                            </button>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </td>
                    }

                    if (_showFillColor)
                    {
                        lastCellWidth -= 33;
                        <td style="width:33%" class="border-0 p-0 m-0 align-middle text-center">
                            <table class="table border-0 p-0 m-0">
                                <tbody>
                                    <tr>
                                        <td class="border-0 p-0 m-0 align-middle text-center">
                                            |
                                        </td>
                                        <td class="border-0 p-0 m-0 align-middle text-center">
                                            <button type="button"
                                                @onclick="@(e => ChangeFillColor("07090d"))"
                                            class="bntRectangleFillStyle btn m-0 p-0"
                                            style="font-size: 2.25rem;color: #07090d">
                                                &bull;
                                            </button>
                                        </td>
                                        <td class="border-0 p-0 m-0 align-middle text-center">
                                            <button type="button"
                                                @onclick="@(e => ChangeFillColor("2780b6"))"
                                            class="bntRectangleFillStyle btn m-0 p-0 border border-dark"
                                            style="font-size: 2.25rem;color: #2780b6">
                                                &bull;
                                            </button>
                                        </td>
                                        <td class="border-0 p-0 m-0 align-middle text-center">
                                            <button type="button"
                                                @onclick="@(e => ChangeFillColor("c2372b"))"
                                            class="bntRectangleFillStyle btn m-0 p-0"
                                            style="font-size: 2.25rem;color: #c2372b">
                                                &bull;
                                            </button>
                                        </td>
                                        <td class="border-0 p-0 m-0 align-middle text-center">
                                            <button type="button"
                                                @onclick="@(e => ChangeFillColor("26ad5c"))"
                                            class="bntRectangleFillStyle btn m-0 p-0 "
                                            style="font-size: 2.25rem;color: #26ad5c">
                                                &bull;
                                            </button>
                                        </td>
                                        <td class="border-0 p-0 m-0 align-middle text-center">
                                            <button type="button"
                                                @onclick="@(e => ChangeFillColor("f6c812"))"
                                            class="bntRectangleFillStyle btn m-0 p-0"
                                            style="font-size: 2.25rem;color: #f6c812">
                                                &bull;
                                            </button>
                                        </td>
                                        <td class="border-0 p-0 m-0 align-middle text-center">
                                            <button type="button"
                                                @onclick="@(e => ChangeFillColor("fbf9f9"))"
                                            class="bntRectangleFillStyle btn m-0 p-0"
                                            style="font-size: 2.25rem;color: #fbf9f9">
                                                &bull;
                                            </button>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </td>
                    }
                }
                <td style="width:@lastCellWidth%" class="border-0">
                </td>
            </tr>
        </tbody>
    </table>
</div>